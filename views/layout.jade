doctype html
html(ng-app='Map')
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/topojson/1.6.9/topojson.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.5/angular.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.5/angular-resource.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.5/angular-route.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.17/d3.js')
    script(src='https://d3js.org/topojson.v1.min.js')
    link(href='/scripts/nv.d3.css' rel='stylesheet')
    script(src='/scripts/nv.d3.js')
    //appjs
    script(src='/maps/map.js')
    //controllers
    script(src='/maps/mapController.js')
    script(src='/maps/donutChartController.js')
    //config
    script(src='/maps/mapConfig.js')
    //directives
    script(src='/maps/mapDirective.js')
    script(src='/maps/DonutChartDirective.js')
    //services
    script(src='/maps/mapService.js')
    script(src='/maps/chartService.js')
    link(rel='stylesheet',href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
  body(ng-controller="mapCtrl")
    block content
